@page
@model MyApp.Namespace.ListaVeterinariosModel
@{
}
<h1>Gestionar Veterinarios</h1>
<a class="btn btn-primary table-btn" asp-page="./CrearVeterinario" >Crear Medico</a>
<table class="table table-dark">
    <tr>
        <th scope="col">Cédula</th>
        <th scope="col">Nombre</th>
        <th scope="col">Apellido</th>
        <th scope="col">Tarjeta Profesional</th>
        <th scope="col">Jornada Laboral</th>
        <th scope="col">Acciones</th>
        
    </tr>
    @foreach(var veterinario in Model.veterinarios){
        <tr>
            <td>@veterinario.cedula</td>
            <td>@veterinario.Nombre</td>
            <td>@veterinario.Apellido</td>
            <td>@veterinario.Tarjeta_Profesional</td>
            <td>@veterinario.Jornada_Laboral</td>
            <td><a class="btn btn-primary table-btn" asp-page="./DetalleVeterinario" asp-route-cedula="@veterinario.cedula">Detalles</a>
                <a class="btn btn-primary table-btn" asp-page="./EditarVeterinario" asp-route-cedula="@veterinario.cedula">Editar</a><br>
        </tr>
    }
</table>